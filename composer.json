{
    "name": "sweetchuck/robo-cdd",
    "description": "Robo task wrapper for the Circular Dependency Detector",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "sweetchuck/git-hooks": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=8.1",
        "consolidation/robo": "^4.0",
        "sweetchuck/cdd": "^0.1"
    },
    "require-dev": {
        "ext-json": "*",
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "nuvoleweb/robo-config": "^3.0",
        "phpmd/phpmd": "^2.10",
        "phpmetrics/phpmetrics": "^2.7",
        "squizlabs/php_codesniffer": "^3.6",
        "sweetchuck/codeception-module-robo-task-runner": "3.x-dev",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "3.x-dev",
        "sweetchuck/robo-phpcs": "3.x-dev",
        "sweetchuck/robo-phpmd": "3.x-dev",
        "symfony/error-handler": "^5.2 || ^6.0",
        "symfony/filesystem": "^5.4 || ^6.0",
        "symfony/finder": "^5.2 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\Robo\\cdd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\Robo\\cdd\\Test\\Helper\\": "tests/_support/Helper/",
            "Sweetchuck\\Robo\\cdd\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\Robo\\cdd\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "extra": {},
    "scripts": {},
    "scripts-descriptions": {}
}
